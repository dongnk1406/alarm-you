{
  "name": "AlarmYou",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "start": "react-native start",
    "test": "jest",
    "format": "prettier \"src/**/*.{ts,tsx,js,jsx}\" --write",
    "pod": "react-native setup-ios-permissions && cd ios && pod install && cd ..",
    "android-clean": "cd android && ./gradlew clean && cd ..",
    "android-build-apk-develop": "rm -rf build && rm -rf android/build && cd android && ./gradlew assembleDevRelease && cd ..",
    "adb-reactotron": "adb reverse tcp:9090 tcp:9090 && adb reverse tcp:3000 tcp:3000 && adb reverse tcp:9001 tcp:9001 && adb reverse tcp:8081 tcp:8081",

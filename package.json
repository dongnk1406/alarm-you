{
  "name": "AlarmYou",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "start": "react-native start",
    "test": "jest",
    "format": "prettier \"src/**/*.{ts,tsx,js,jsx}\" --write",
    "pod": "react-native setup-ios-permissions && cd ios && bundle exec pod install && cd ..",
    "android-clean": "rm -rf android/build && cd android && ./gradlew clean && cd ..",
    "android-build-apk-develop": "rm -rf build && rm -rf android/build && cd android && ./gradlew clean && ./gradlew assembleRelease && cd ..",
    "adb-reactotron": "adb reverse tcp:9090 tcp:9090 && adb reverse tcp:3000 tcp:3000 && adb reverse tcp:9001 tcp:9001 && adb reverse tcp:8081 tcp:8081",
    "prepare": "husky install",
    "bootstrap": "yarn install && bundle install && yarn pod",
    "clean": "watchman watch-del-all && yarn cache clean && rm -rf ios/build && pod cache clean --all && yarn android-clean",
    "clean-all": "yarn clean && rm -rf node_modules && rm -rf package-lock.json && rm -rf yarn.lock && rm -rf ios/Podfile.lock && rm -rf ios/Pods && rm -rf vendor",
    "prepare-build-ios": "react-native bundle --entry-file index.js --platform ios --dev false --bundle-output ios/main.jsbundle --assets-dest ios",
    "cp-android": "appcenter codepush release-react -a Socar-Mobility-Malaysia-Sdn.-Bhd./trevo-android --description $changelog -d Staging -m --description 'Modified the header color' ",
    "cp-android:staging": "appcenter codepush release-react -a dong.nguyen-adamo.software/alram-you -m --description 'Staging: Modified home button color'",
    "cp-android:rb": "appcenter codepush rollback -a Socar-Mobility-Malaysia-Sdn.-Bhd./trevo-android Production",
    "cp-android:rb-staging": "appcenter codepush rollback -a Socar-Mobility-Malaysia-Sdn.-Bhd./trevo-android Staging",
    "cp-ios": "appcenter codepush release-react -a Socar-Mobility-Malaysia-Sdn.-Bhd./trevo-ios --description $changelog -d Production --plist-file 'ios/Trevo/Info.plist'",
    "cp-ios:staging": "appcenter codepush release-react -a Socar-Mobility-Malaysia-Sdn.-Bhd./trevo-ios  --description $changelog -d Staging --plist-file 'ios/Trevo/Info.plist'",
    "cp-ios:rb": "appcenter codepush rollback -a Socar-Mobility-Malaysia-Sdn.-Bhd./trevo-ios Production",
    "cp-ios:rb-staging": "appcenter codepush rollback -a dong.nguyen-adamo.software/alram-you Staging",
    "android:bundle": "yarn android-clean && cd android && ./gradlew bundleRelease && cd .."
  },
